@model List<Project.Common.Entities.Product>
@*use Model not model in view*@

@{
    var listHotProducts = new List<Project.Common.Entities.Product>();
    var listNewProducts = new List<Project.Common.Entities.Product>();

    foreach (var item in Model)
    {
        if (item.Trend == Project.Common.Enum.Trend.HotProduct)
        {
            listHotProducts.Add(item);
        }
        if (item.Trend == Project.Common.Enum.Trend.NewProduct)
        {
            listNewProducts.Add(item);
        }
    }
}

